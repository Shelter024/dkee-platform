{
  "name": "dk-executive-engineers",
  "version": "1.0.0",
  "description": "Integrated platform for DK Executive Engineers - Automotive and Property Management",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "analyze": "ANALYZE=true npm run build",
    "secrets": "node scripts/generate-secrets.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "anonymize:staging": "ts-node --compiler-options {\"module\":\"CommonJS\"} scripts/anonymize-staging.ts",
    "mobile:customer": "powershell -NoProfile -Command \"cd ./mobile/customer-app; npm i; npm run start\"",
    "mobile:staff": "powershell -NoProfile -Command \"cd ./mobile/staff-app; npm i; npm run start\""
  },
